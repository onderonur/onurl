{
  "name": "onurl",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "codequality:check": "pnpm format:check && pnpm lint:check && pnpm types:check",
    "codequality:fix": "pnpm format:fix && pnpm lint:fix && pnpm types:check",
    "db:migrate": "dotenv -e .env.development -- prisma db push",
    "dev": "next dev",
    "format:check": "prettier --check --ignore-unknown .",
    "format:fix": "prettier --write --ignore-unknown .",
    "postinstall": "prisma generate",
    "lint:check": "next lint --max-warnings 0",
    "lint:fix": "next lint --fix --max-warnings 0",
    "postinstall:about": "echo 'https://www.prisma.io/docs/guides/other/troubleshooting-orm/help-articles/vercel-caching-issue'",
    "prepare": "husky",
    "start": "next start",
    "types:check": "tsc",
    "unused:check": "knip",
    "updates:check": "npm-check-updates -i"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "file-saver": "^2.0.5",
    "go-try": "^3.0.2",
    "nanoid": "^5.0.7",
    "next": "^14.2.15",
    "qrcode.react": "^4.0.1",
    "react": "^18.3.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-share": "^5.1.0",
    "server-only": "^0.0.1",
    "tailwind-merge": "^2.5.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/node": "^22.7.5",
    "@types/qrcode.react": "^1.0.5",
    "@types/react": "^18.3.11",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.15",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "husky": "^9.1.6",
    "knip": "^5.33.3",
    "lint-staged": "^15.2.10",
    "npm-check-updates": "^17.1.3",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "prisma": "^5.20.0",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.3"
  },
  "packageManager": "pnpm@9.12.2"
}
